  "Aws": {
    "S3": {
      "AccessKey": "[YourS3AccessKey]",
      "SecretKey": "[YourS3SecretKey]",
      "BucketName": "[YourS3BucketName]",
      "Region": "[YourS3Region]"
    }
  }

}
