{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "YourAzureInstrumentationKey"
  },
  "MongodbConnectionString": "mongodb://localhost:27017",
  "SQLServerConnectionString": "Server=(localdb)\\TEST;Database=Test;user id=sa;password=password;Integrated Security=True;",
